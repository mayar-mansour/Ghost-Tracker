{
    "name": "mayar/ghosttrack",
    "description": "Log user actions silently in Laravel.",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Mayar\\GhostTrack\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10|^11"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mayar\\GhostTrack\\GhostTrackServiceProvider"
            ],
            "aliases": {
                "GhostTrack": "Mayar\\GhostTrack\\Facades\\GhostTrack"
            }
        }
    }
}
